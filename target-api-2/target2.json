import requests

endpoint = "todos"
url = f"https://jsonplaceholder.typicode.com/{endpoint}"

"""## GET"""

response = requests.get(url)

response.headers

response.status_code

result = response.json()

new_user = []
for x in range(len(result)):
  if x < 5:
    new_user.append(result[x])
for y in new_user:
  print(y)

response.json()

result = response.json()

completed_true = 0
completed_false = 0

for user in result:
  if user["completed"] is False:
    completed_false = completed_false + 1
  else:
    completed_true = completed_true + 1

print(completed_true)
print(completed_false)

#mengetahui panjang range index
for index in range(len(result)):
  print(result[index])

#mengetahui panjang result user
for user in result:
  print(user)

response.json()

response.json()[0]

"""## POST"""

todo = {"userId": 1, "title": "Buy milk", "completed": False, "id": 3}

response = requests.post(url, json=todo)

response.status_code

response.headers

response.json()

"""## PUT"""

todo = {"userId": 99, "id": 1, "title": "new title", "completed": True}

endpoint = "todos"
param_id = "1"
url = f"https://jsonplaceholder.typicode.com/{endpoint}/{param_id}"

response = requests.put(url, json=todo)

response.status_code

response.json()

"""## PATCH"""

todo = {"title": "Mow lawn"}

endpoint = "todos"
param_id = "1"
url = f"https://jsonplaceholder.typicode.com/{endpoint}/{param_id}"

response = requests.patch(url, json=todo)

response.status_code

response.json()

"""## DELETE"""

endpoint = "todos"
param_id = "1"
url = f"https://jsonplaceholder.typicode.com/{endpoint}/{param_id}"

response = requests.delete(url)

response.status_code

response.json()